{
  "name": "voyage-ai-service",
  "version": "1.0.0",
  "description": "AI-powered travel itinerary generation microservice",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
  "smoke": "ts-node-dev --transpile-only src/scripts/smoke.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "ai",
    "travel",
    "itinerary",
    "microservice",
    "openai",
    "gpt"
  ],
  "author": "Voyage Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonrepair": "^3.5.1",
    "morgan": "^1.10.0",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
